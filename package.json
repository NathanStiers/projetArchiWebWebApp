{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve"
  },
  "dependencies": {
    "@meforma/vue-toaster": "^1.2.2",
    "axios": "^0.21.1",
    "core-js": "^3.9.1",
    "lodash": "^4.17.21",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "vue": "^3.0.7",
    "vue-final-modal": "^3.0.1",
    "vue-loader-v16": "^16.0.0-beta.5.4",
    "vue-router": "^4.0.0-0",
    "vue-simple-drawer": "^1.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "^4.5.11",
    "@vue/compiler-sfc": "^3.0.7",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "vue"
    ],
    "extends": [
      "plugin:vue/vue3-essential",
      "plugin:vue/vue3-recommended",
      "eslint:recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "rules": {
      "vue/max-attributes-per-line": ["error", {
        "singleline": 4,
        "multiline": {
          "max": 6,
          "allowFirstLine": true
        }
      }]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
